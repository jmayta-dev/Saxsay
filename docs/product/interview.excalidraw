    {
      "id": "Xj4Uws4pKeene7Hx8mtZ-",
      "type": "text",
      "x": 218.85714285714312,
      "y": 1506.793247823163,
      "width": 743,
      "height": 275,
      "angle": 0,
      "strokeColor": "#1e1e1e",
